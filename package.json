{
  "name": "di-resolver",
  "version": "1.0.8",
  "description": "typescript dependency injection resolver without reflect-metadata",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "browserBazel": "dist/index.umd.bazel.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/moczix/di-resolver.git"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "example": "ts-node-dev --respawn example/index.ts",
    "test": "node test/test.js",
    "pretest": "npm run build"
  },
  "keywords": [
    "di",
    "dependency",
    "injection",
    "ioc",
    "dependency"
  ],
  "author": "moczniak",
  "license": "MIT",
  "devDependencies": {
    "reflect-metadata": "^0.1.13",
    "rollup": "^1.19.4",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.1",
    "rollup-plugin-typescript": "^1.0.1",
    "ts-node-dev": "^1.0.0-pre.41",
    "tslib": "^1.10.0",
    "typescript": "^3.5.3"
  }
}
